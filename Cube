<html>
 <body>
  <table>
  <tr>
	<td><input type="text" id="new" placeholder="enter number"></td>
  </tr>
  <tr>
	<td><button onclick="cube()">submit</button></td>
  </tr>
  </table>
 <div id="num"></div>
 </body>
<script>
function cube()
{
	var a,cube;
     a=parseInt(document.getElementById("new").value);
	 cube=a*a*a;
	 document.getElementById("num").innerHTML ="cube of" +a+ ":"+cube;
}
</script>
</html>
